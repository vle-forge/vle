<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE vle_project PUBLIC "-//VLE TEAM//DTD Strict//EN" "http://www.vle-project.org/vle-1.0.0.dtd">
<vle_project version="1.1.0" date="mar., 12 janv. 2010" author="Gauthier Quesnel">
<structures>
<model name="Top model" type="coupled" x="0" y="0" width="459" height="301"  >
<submodels>
<model name="Agent" type="atomic" dynamics="agent" observables="obs" x="140" y="25" width="100" height="45" >
<out>
 <port name="out" />
</out>
</model>
<model name="counter" type="atomic" dynamics="counter" observables="storage" x="140" y="85" width="100" height="45" >
<in>
 <port name="in" />
</in>
</model>
</submodels>
<connections>
<connection type="internal">
 <origin model="Agent" port="out" />
 <destination model="counter" port="in" />
</connection>
</connections>
</model>
</structures>
<dynamics>
<dynamic name="agent" library="decision" model="OnlyAgentPrecedenceConstraint" type="local"  />
<dynamic name="counter" library="decision" model="Counter" type="local"  />
</dynamics>
<experiment name="Only" duration="10.000000000000000" begin="0.000000000000000" combination="linear" seed="10" >
<views>
<outputs>
<output name="output" location="" format="local" plugin="file" >
<map><key name="output"><string>out</string></key></map></output>

<output name="storage" location="" format="local" plugin="storage" />

</outputs>
<observables>
<observable name="obs" >
<port name="text" >
 <attachedview name="output" />
</port>

</observable>

<observable name="storage" >
<port name="counter" >
 <attachedview name="storage" />
</port>

</observable>

</observables>
<view name="output" output="output" type="timed" timestep="1.000000000000000" />

<view name="storage" output="storage" type="timed" timestep="1.000000000000000" />

</views>
</experiment>
</vle_project>
